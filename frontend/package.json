{
  "name": "@parity/substrate-playground-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "lint": "npx eslint ./src/ --ext .js,.jsx,.jsx,.ts,.tsx",
    "dev": "parcel dev public/index.html",
    "watch": "parcel watch public/index.html",
    "build": "parcel build public/index.html && mkdir -p dist/assets/ && cp -R public/assets/* dist/assets/ && cp public/robots.txt dist/",
    "clean": "rm -rf node_modules/ dist/ .cache/ yarn.lock"
  },
  "dependencies": {
    "@material-ui/core": "4.9.14",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.53",
    "@xstate/react": "0.8.1",
    "history": "4.10.1",
    "marked": "1.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-spring": "8.0.27",
    "uuid": "8.0.0",
    "xstate": "4.9.1"
  },
  "devDependencies": {
    "@pollyjs/adapter-fetch": "4.3.0",
    "@pollyjs/core": "4.3.0",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@types/uuid": "8.0.0",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "eslint": "6.8.0",
    "eslint-plugin-react": "7.20.0",
    "parcel-bundler": "1.12.4",
    "typescript": "3.9.2"
  }
}
