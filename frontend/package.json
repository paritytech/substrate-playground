{
  "name": "@parity/substrate-playground-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "lint": "npx eslint ./src/ --ext .js,.jsx,.jsx,.ts,.tsx",
    "dev": "parcel dev public/index.html --port 80",
    "watch": "parcel watch public/index.html",
    "build": "parcel build public/index.html && mkdir -p dist/assets/ && cp -R public/assets/* dist/assets/ && cp public/robots.txt dist/",
    "test": "ava",
    "clean": "rm -rf node_modules/ dist/ .cache/ yarn.lock"
  },
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.56",
    "@substrate/playground-api": "1.0.0",
    "@xstate/react": "1.0.0",
    "broadcast-channel": "3.2.0",
    "history": "4.10.1",
    "jpeg-js": "0.4.2",
    "marked": "1.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-spring": "8.0.27",
    "uuid": "8.3.0",
    "xstate": "4.13.0"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@pollyjs/adapter-fetch": "5.0.0",
    "@pollyjs/core": "5.0.0",
    "@types/node": "14.11.2",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "ava": "^3.13.0",
    "eslint": "6.8.0",
    "eslint-plugin-react": "7.21.2",
    "parcel-bundler": "1.12.4",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  }
}
