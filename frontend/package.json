{
  "name": "@parity/substrate-playground-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "lint": "npx eslint ./src/ --ext .js,.jsx,.jsx,.ts,.tsx",
    "dev": "parcel dev public/index.html",
    "watch": "parcel watch public/index.html",
    "build": "parcel build public/index.html && mkdir -p dist/assets/ && cp -R public/assets/* dist/assets/ && cp public/robots.txt dist/",
    "test": "node test/index.js",
    "itest": "mocha itest/index.js",
    "clean": "rm -rf node_modules/ dist/ .cache/ yarn.lock"
  },
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.56",
    "@xstate/react": "0.8.1",
    "history": "4.10.1",
    "jpeg-js": "0.4.2",
    "marked": "1.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-spring": "8.0.27",
    "uuid": "8.3.0",
    "xstate": "4.13.0"
  },
  "devDependencies": {
    "@pollyjs/adapter-fetch": "5.0.0",
    "@pollyjs/core": "5.0.0",
    "@types/node": "14.6.1",
    "@types/react": "16.9.48",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "eslint": "6.8.0",
    "eslint-plugin-react": "7.20.6",
    "expect.js": "^0.3.1",
    "mocha": "^8.1.2",
    "node-fetch": "^2.6.0",
    "parcel-bundler": "1.12.4",
    "playwright": "1.3.0",
    "typescript": "4.0.2"
  }
}
